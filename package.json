{
  "name": "feathers-rethinky",
  "version": "0.0.1",
  "description": "Thinky.js RethinkDB Adaptor for Feathers JS",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src test --no-color",
    "babel": "rm -rf lib && mkdir lib && babel src/index.js -o lib/index.js",
    "test": "npm run lint && npm run mocha:${CIRCLECI}",
    "mocha:true": "mocha -R xunit --compilers js:babel-register  > $CIRCLE_TEST_REPORTS/phpunit/junit.xml",
    "mocha:": "mocha  --no-colors -R spec --compilers js:babel-register",
    "prepublish": "npm run babel"
  },
  "files": ["src","lib"],
  "keywords": [
    "Rethink",
    "feathers",
    "thinky"
  ],
  "publishConfig": {
    "registry":"https://registry.npmjs.org/"
  },
  "author": "Miles Wilson <miles@mtwilson.co.uk>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^1.10.3",
    "feathers": "^1.2.0",
    "feathers-service-tests": "^0.5.3",
    "isparta": "^4.0.0",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3"
  },
  "dependencies": {
    "feathers-errors": "^1.1.6",
    "feathers-query-filters": "^1.5.1",
    "thinky": "^2.2.4",
    "uberproto": "^1.2.0"
  }
}
